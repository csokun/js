{
    "compileOnSave": true,
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true
    },
    "filesGlob": [
        "src/*.ts",
        "src/*.tsx"
    ],
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}
